pipeline {
  agent any 
    stages {
      stage ('BUILD') {
        steps {
               echo "This is build stage by webhook123 and only for this?"
            
         }
        }
        stage ('DEPLOY') {
          steps {
             echo "This is deploy stage by webhook123"
                
          }
        }
          stage ('TEST') {
            steps {
               echo "This is a Test stage by webhook123"
                
             }
           }
         }
      }
         
                
