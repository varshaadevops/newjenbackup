pipeline {
  agent any 
    stages {
      stage ('BUILD') {
        steps {
               echo "This is build stage"
            
         }
        }
        stage ('DEPLOY') {
          steps {
             echo "This is deploy stage"
                
          }
        }
          stage ('TEST') {
            steps {
               echo "This is a Test stage"
                
             }
           }
         }
      }
         
                
